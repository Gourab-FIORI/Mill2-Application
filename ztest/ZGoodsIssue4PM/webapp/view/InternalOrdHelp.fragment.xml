<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="{i18n>InternalOrd}" contentWidth="50%" contentHeight="70%">
		<Table id="idTableIO" items="{/InternalOrdSet}" growing="true" growingScrollToLoad="true" mode="SingleSelectMaster" selectionChange="onRowSelectIO">
			<columns>
				<Column><Text text="{i18n>InternalOrd}"/></Column>
				<Column><Text text="{i18n>description}"/></Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells><Text text="{Aufnr}"/></cells>
					<cells><Text text="{Ktext}"/></cells>
				</ColumnListItem>
			</items>
		</Table>
		<endButton>
			<Button text="Close" type="Emphasized" press="onCloseDialogIO"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>